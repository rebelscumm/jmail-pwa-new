{
  "name": "m3-svelte",
  "version": "5.1.0",
  "engines": {
    "node": ">=20.19.0"
  },
  "license": "Apache-2.0 OR GPL-3.0-only",
  "repository": "KTibow/m3-svelte",
  "author": {
    "name": "KTibow"
  },
  "homepage": "https://github.com/KTibow/m3-svelte",
  "funding": "https://github.com/sponsors/KTibow",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "vite dev",
    "check-stale-summaries": "node ./scripts/check-stale-summaries.js",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package -o package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && eslint",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "files": [
    "package",
    "!package/**/*.test.*",
    "!package/**/*.spec.*"
  ],
  "svelte": "./package/index.js",
  "types": "./package/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./package/index.d.ts",
      "svelte": "./package/index.js"
    },
    "./misc/styles.css": {
      "import": "./package/misc/styles.css"
    },
    "./misc/recommended-styles.css": {
      "import": "./package/misc/recommended-styles.css"
    }
  },
  "dependencies": {
    "@iconify/types": "^2.0.0",
    "@ktibow/iconset-material-symbols": "^0.0.1752989765",
    "@ktibow/material-color-utilities-nightly": "^0.3.11753062248503",
    "idb": "^8.0.0",
    "luxon": "^3.5.0",
    "svelte": "^5.37.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "vitest": "^1.0.0",
    "happy-dom": "^10.0.0",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.31.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.25.1",
    "@sveltejs/package": "^2.4.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "fast-glob": "^3.3.3",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.12",
    "svelte-check": "^4.3.0",
    "svelte-highlight": "^7.8.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.5"
  },
  "keywords": [
    "svelte",
    "material",
    "material 3",
    "material you",
    "m3",
    "ui",
    "frontend",
    "design-system",
    "ui-library",
    "theming"
  ],
  "sideEffects": false,
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
